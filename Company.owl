<Ontology xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://localhost:9000/Company"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     ontologyIRI="http://localhost:9000/Company">
    <Prefix name="owl" IRI="http://www.w3.org/2002/07/owl#"/>
    <Prefix name="rdf" IRI="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
    <Prefix name="xml" IRI="http://www.w3.org/XML/1998/namespace"/>
    <Prefix name="xsd" IRI="http://www.w3.org/2001/XMLSchema#"/>
    <Prefix name="rdfs" IRI="http://www.w3.org/2000/01/rdf-schema#"/>
    <Import>http://localhost:9000/RepresentanteLegal.owl</Import>
    <!-- Declaraci贸n de Clases -->
    <Declaration>
        <Class IRI="#Company"/>
    </Declaration>
    

    <!-- Declaraci贸n de Propiedades de Datos -->
    <Declaration>
        <DataProperty IRI="#hasName"/>
    </Declaration>
    <Declaration>
        <DataProperty IRI="#hasEnrollmentNumber"/>
    </Declaration>
    <Declaration>
        <DataProperty IRI="#hasDateOfRenovation"/>
    </Declaration>

    <!-- Declaraci贸n de Propiedades de Objetos -->
    <Declaration>
        <ObjectProperty IRI="#hasLegalRepresentative"/>
    </Declaration>
     <Declaration>
        <ObjectProperty IRI="#hasSustituteRepresentative"/>
    </Declaration>

    <!-- Definici贸n de Propiedades de Datos -->
    <DataPropertyDomain>
        <DataProperty IRI="#hasName"/>
        <Class IRI="#Company"/>
    </DataPropertyDomain>
    <DataPropertyDomain>
        <DataProperty IRI="#hasEnrollmentNumber"/>
        <Class IRI="#Company"/>
    </DataPropertyDomain>
    <DataPropertyDomain>
        <DataProperty IRI="#hasDateOfRenovation"/>
        <Class IRI="#Company"/>
    </DataPropertyDomain>
    <DataPropertyRange>
        <DataProperty IRI="#hasName"/>
        <Datatype abbreviatedIRI="xsd:string"/>
    </DataPropertyRange>
    <DataPropertyRange>
        <DataProperty IRI="#hasEnrollmentNumber"/>
        <Datatype abbreviatedIRI="xsd:string"/>
    </DataPropertyRange>
    <DataPropertyRange>
        <DataProperty IRI="#hasDateOfRenovation"/>
        <Datatype abbreviatedIRI="xsd:integer"/>
    </DataPropertyRange>

    <ObjectPropertyDomain>
        <ObjectProperty IRI="#hasLegalRepresentative"/>
        <Class IRI="#Company"/>
    </ObjectPropertyDomain>

    <ObjectPropertyDomain>
        <ObjectProperty IRI="#hasSustituteRepresentative"/>
        <Class IRI="#Company"/>
    </ObjectPropertyDomain>
      <ObjectPropertyRange>
        <ObjectProperty IRI="#hasLegalRepresentative"/>
        <Class IRI="http://localhost:9000/RepresentanteLegal#RepresentanteLegal"/>
    </ObjectPropertyRange>
    <ObjectPropertyRange>
        <ObjectProperty IRI="#hasSustituteRepresentative"/>
        <Class IRI="http://localhost:9000/RepresentanteSuplente#RepresentanteSuplente"/>
    </ObjectPropertyRange>
  
</Ontology>
